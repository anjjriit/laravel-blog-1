{
    "name": "sebastiaanluca/laravel-blog",
    "type": "library",
    "description": "A simple-to-use blog system you can pull into any existing Laravel project and immediately get writing.",
    "keywords": [
        "Laravel",
        "Blog",
        "Sebastiaan Luca"
    ],
    "homepage": "https://www.github.com/sebastiaanluca/laravel-blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "https://www.sebastiaanluca.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7.0",
        "graham-campbell/markdown": "^7.0",
        "illuminate/support": "^5.1",
        "laravelcollective/html": "^5.3",
        "nesbot/carbon": "^1.21",
        "rinvex/repository": "dev-feature/add-entities as 2.0.1",
        "sebastiaanluca/laravel-helpers": "dev-develop",
        "sebastiaanluca/laravel-router": "dev-develop as 2.0.0",
        "sebastiaanluca/laravel-unbreakable-migrations": "dev-develop",
        "sebastiaanluca/laravel-validator": "dev-develop"
    },
    "require-dev": {
        "nwidart/laravel-modules": "dev-feature/refactor",
        "phpunit/phpunit": "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:sebastiaanluca/laravel-modules.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sebastiaanluca/repository.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\Blog\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
